<html>
<head>
<title>{{ self.title() }} | Telegram Hub</title>
<script type="text/javascript" src="{{PUBLIC_STATIC_URL_PREFIX}}/jquery-3.5.1.min.js"></script> 
<script type="text/javascript" src="{{PUBLIC_STATIC_URL_PREFIX}}/common.js"></script> 
</head>
<body>
<h1>{{ self.title() }}</h1>
{% block body %}{% endblock %}
<hr/>
{% if current_user.is_authenticated %}
 <p><a href="/logout">Logout</a> <code>USER_ID={{current_user.id}}</code></p>
{% endif %}
{% block footer %}{% endblock %}
<div style="display:none"><script>
{% block script %}{% endblock %}
</script></div>
</body>
</html>
